    </view>
    <view class='m-steps-cont'>
      <view class='u-rank'>2</view>
      <view class='m-steps-tips'>
        <text>提现成功</text>
      </view>
    </view>
    <view class='m-steps-cont'>
      <view class='u-rank'>2</view>
      <view class='m-steps-tips'>
        <text>提现成功</text>
        <text>提现成功</text>
      </view>
    </view>
    <view class='m-steps-cont'>
      <view class='u-rank'>2</view>
      <view class='m-steps-tips'>
        <text>提现成功</text>
      </view>
    </view>
    <view class='u-steps-solid'></view>
  </view>
</view>

<view class='m-submit-btn' bindtap='back'>返回结算中心</view>
