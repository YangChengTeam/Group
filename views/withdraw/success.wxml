<!--views/withdraw/success.wxml-->
<view class='g-cont-top'>
  <image src='/images/me/icon-5.png'></image>  
  <text>申请提现成功</text>
</view>
<image src='/images/me/bg-9.png' class='u-bg-3'></image>
<view class='g-cont-foot'>
  <view class='m-steps-box'>
    <view class='m-steps-cont'>
      <view class='u-rank'>1</view>
      <view class='m-steps-tips'>
        <text>提现申请成功</text>
        <text>提现申请将在24小时内处理</text>
