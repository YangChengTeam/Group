page{ background: #f3f5fb;}

.g-box{ width: 100%; height: auto; padding: 40rpx 30rpx; box-sizing: border-box; display: block; overflow: hidden; background: #fff; margin: 20rpx 0 0;}
.g-box text{ width: 100%; height: auto; font-size: 26rpx; font-weight: normal; color: #5a5e64; display: block; overflow: hidden}

.g-box text:nth-child(2){ margin: 20rpx 0 0; font-size: 58rpx; font-weight: bold; color: #333;}

.m-submit-btn{ width: 688rpx; height: 90rpx; line-height: 90rpx; font-size: 32rpx; font-weight: normal; color: #fff; text-align: center; border-radius: 44rpx; background: linear-gradient(to top right, #ff2741 , #ff6377); box-shadow: 0px 6rpx 8rpx #f89dab; display: block; overflow: hidden; margin: 80rpx auto 0;}

