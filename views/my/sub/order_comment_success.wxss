/* views/my/sub/order_comment_success.wxss */

page {
  background: #eee;
  padding: 0 0 0 0;
}

.u-top-bg {
  width: 750rpx;
  height: 436rpx;
  display: block;
  overflow: hidden;
  position: absolute;
  left: 0;
  top: 0;
}

.g-white-box {
  width: 100%;
  height: auto;
  background: #fff;
  display: block;
  overflow: hidden;
}

.g-white-box .u-img {
  width: 212rpx;
  height: 181rpx;
  margin: 78rpx auto 0;
  display: block;
  overflow: hidden;
}

.g-white-box .u-t1 {
  width: 100%;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 32rpx;
  font-weight: normal;
  color: #000;
  text-align: center;
  margin: 40rpx 0 0;
  display: block;
  overflow: hidden;
}

.g-white-box .u-t2 {
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 36rpx;
  font-weight: bold;
  color: #ff2741;
  text-align: center;
  margin: 24rpx 0 0;
  display: block;
  overflow: hidden;
}

.g-white-box .m-save-btn {
  width: 100%;
  height: auto;
  padding: 20rpx 0 45rpx;
  box-sizing: border-box;
  display: block;
  overflow: hidden;
}

.g-white-box .m-save-btn view {
  width: 550rpx;
  height: 80rpx;
  line-height: 76rpx;
  font-size: 28rpx;
  font-weight: normal;
  color: #ff2741;
  text-align: center;
  border: 2rpx solid #ff2741;
  box-sizing: border-box;
  margin: 24rpx auto 0;
  display: block;
  overflow: hidden;
  border-radius: 8rpx;
}

.g-white-box .m-save-btn .u-savehb-btn {
  background: #ff2741;
  color: #fff;
}

.g-white-box .m-gethb-box {
  width: 100%;
  height: auto;
  padding: 0 0 45rpx 0;
  box-sizing: border-box;
  display: block;
  overflow: hidden;
}

.g-white-box .m-gethb-box .m-gethb-cont {
  width: 263rpx;
  height: 161rpx;
  background: linear-gradient(to right, #ff5974, #fb8668);
  float: left;
  margin: 30rpx 25rpx 0 60rpx;
  display: inline;
  border-radius: 8rpx;
}

.g-white-box .m-gethb-box .m-gethb-cont .m-top {
  width: 100%;
  height: auto;
  display: block;
  margin: 12rpx 0 0;
  position: relative;
}

.g-white-box .m-gethb-box .m-gethb-cont .m-top text {
  width: auto;
  height: 32rpx;
  line-height: 32rpx;
  font-size: 20rpx;
  font-weight: normal;
  color: #77341a;
  text-indent: 13rpx;
  display: block;
  overflow: hidden;
  position: relative;
  z-index: 5;
}

.g-white-box .m-gethb-box .m-gethb-cont .m-top image {
  width: 151rpx;
  height: 41rpx;
  display: block;
  overflow: hidden;
  position: absolute;
  left: -3rpx;
  top: 0;
  z-index: 0;
}

.g-white-box .m-gethb-box .m-gethb-cont .m-middle {
  width: 100%;
  height: auto;
  display: -webkit-box;
  overflow: hidden;
  margin: 12rpx 0 0;
}

.g-white-box .m-gethb-box .m-gethb-cont .m-middle .m-middle-left {
  width: auto;
  height: auto;
  line-height: 50rpx;
  font-size: 24rpx;
  font-weight: normal;
  color: #fff;
  text-align: right;
  display: block;
  overflow: hidden;
  -webkit-box-flex: 1;
}

.g-white-box .m-gethb-box .m-gethb-cont .m-middle .m-middle-left text {
  font-size: 48rpx;
  font-weight: bold;
}

.g-white-box .m-gethb-box .m-gethb-cont .m-middle .m-middle-right {
  width: 122rpx;
  height: auto;
  margin: 0 0 0 12rpx;
  display: block;
  overflow: hidden;
}

.g-white-box .m-gethb-box .m-gethb-cont .m-middle .m-middle-right .u-mr-t1 {
  width: 100%;
  height: 26rpx;
  line-height: 26rpx;
  font-size: 20rpx;
  font-weight: normal;
  color: #fff;
  display: block;
  overflow: hidden;
}

.g-white-box .m-gethb-box .m-gethb-cont .m-middle .m-middle-right .u-mr-t2 {
  width: 100%;
  height: 26rpx;
  line-height: 26rpx;
  font-size: 20rpx;
  font-weight: normal;
  color: #fff;
  display: block;
  overflow: hidden;
}

.g-white-box .m-gethb-box .m-gethb-cont .m-bottom-btn {
  width: 202rpx;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 20rpx;
  font-weight: normal;
  color: #ff5d73;
  background: #fff100;
  border-radius: 22rpx;
  margin: 4rpx auto 0;
  display: block;
  overflow: hidden;
  text-align: center;
}

/*标题*/

.g-title {
  width: 100%;
  height: auto;
  margin: 20rpx 0 0;
  padding: 40rpx 0 0;
  text-align: center;
  display: block;
  overflow: hidden;
  background: #fff;
}

.g-title .m-text {
  width: auto;
  height: 34rpx;
  line-height: 34rpx;
  font-size: 30rpx;
  font-weight: normal;
  color: #333;
  display: inline-block;
  overflow: hidden;
  padding: 0 60rpx;
  box-sizing: border-box;
  position: relative;
}

.g-title .m-text .u-icon-1 {
  width: 38rpx;
  height: 28rpx;
  display: block;
  overflow: hidden;
  position: absolute;
  left: 0;
  top: 3rpx;
}

.g-title .m-text .u-icon-2 {
  width: 38rpx;
  height: 28rpx;
  display: block;
  overflow: hidden;
  position: absolute;
  right: 0;
  top: 3rpx;
}

/*列表*/

.g-list-box {
  width: 750rpx;
  height: auto;
  margin: 0 auto;
  padding: 0 10rpx 50rpx;
  box-sizing: border-box;
  box-sizing: border-box;
  display: block;
  overflow: hidden;
  background: #fff;
}

.g-list-box .m-cont {
  width: 320rpx;
  height: auto;
  margin: 30rpx 0 0 30rpx;
  box-sizing: border-box;
  float: left;
  display: inline;
  overflow: hidden;
}

.g-list-box .m-cont .m-img {
  width: 100%;
  height: auto;
  display: block;
  overflow: hidden;
  position: relative;
  border-radius: 0;
}

.g-list-box .m-cont .m-img .u-img {
  width: 320rpx;
  height: 320rpx;
  display: block;
  overflow: hidden;
}

.g-list-box .m-cont .m-img text {
  width: auto;
  height: 66rpx;
  line-height: 66rpx;
  font-size: 28rpx;
  font-weight: normal;
  color: #fff;
  padding: 0 14rpx;
  box-sizing: border-box;
  background: rgba(0, 0, 0, 0.5);
  border-radius: 8rpx 0 0 0;
  display: block;
  overflow: hidden;
  position: absolute;
  right: 0;
  bottom: 0;
  z-index: 5;
}

.g-list-box .m-cont .u-title {
  width: 100%;
  height: auto;
  max-height: 88rpx;
  line-height: 48rpx;
  font-size: 28rpx;
  font-weight: normal;
  color: #333;
  margin: 20rpx 0 0;
  display: block;
  overflow: hidden;
}

.g-list-box .m-cont .u-buy {
  width: 100%;
  height: 56rpx;
  line-height: 56rpx;
  display: -webkit-box;
  overflow: hidden;
  margin: 20rpx 0 0;
}

.g-list-box .m-cont .u-buy .u-money-1 {
  width: auto;
  font-size: 40rpx;
  font-weight: bold;
  color: #ff2741;
  display: block;
  overflow: hidden;
}

.g-list-box .m-cont .u-buy .u-money-2 {
  width: auto;
  line-height: 66rpx;
  font-size: 30rpx;
  font-weight: normal;
  color: #d0d0d0;
  text-decoration: line-through;
  margin: 0 0 0 20rpx;
  display: block;
  overflow: hidden;
  -webkit-box-flex: 1;
}
