/* views/good/index.wxss */

page {
  background: #fff;
  padding: 0 0 130rpx 0;
}

.u-banner {
  width: 750rpx;
  height: 215rpx;
  display: block;
  overflow: hidden;
}

.g-tab-btn {
  width: 100%;
  height: auto;
  padding: 40rpx 120rpx 10rpx;
  box-sizing: border-box;
  display: flex;
  overflow: hidden;
}

.g-tab-btn .u-btn {
  width: auto;
  height: auto;
  padding: 0 0 30rpx 0;
  display: block;
  overflow: hidden;
  position: relative;
  flex: 1;
}

.g-tab-btn .u-btn text {
  width: 100%;
  height: auto;
  font-size: 28rpx;
  font-weight: normal;
  color: #333;
  text-align: center;
  display: block;
  overflow: hidden;
}

.g-tab-btn .u-btn .u-icon {
  width: 60rpx;
  height: 9rpx;
  background: #f35052;
  border-radius: 20rpx;
  position: absolute;
  left: 50%;
  bottom: 0;
  transform: translateX(-50%);
  display: none;
  overflow: hidden;
}

.g-tab-btn .u-btn.f-hover text {
  font-weight: bold;
  color: #f35052;
}

.g-tab-btn .u-btn.f-hover .u-icon {
  display: block;
}

.g-label {
  width: 100%;
  height: auto;
  display: block;
  overflow: hidden;
}

.g-label text {
  width: auto;
  height: 55rpx;
  line-height: 55rpx;
  font-size: 26rpx;
  font-weight: normal;
  color: #555;
  background: #efefef;
  border-radius: 55rpx;
  padding: 0 28rpx;
  float: left;
  margin: 30rpx 0 0 30rpx;
  display: inline;
  overflow: hidden;
}

.g-label text.f-hover {
  background: #ffeef0;
  color: #f35052;
}

.g-main-box {
  width: 100%;
  height: auto;
  padding: 30rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #eee;
  display: block;
  overflow: hidden;
}

.g-main-box .m-main-user {
  width: 100%;
  height: auto;
  display: -webkit-box;
  overflow: hidden;
}

.g-main-box .m-main-user .u-face {
  width: 100rpx;
  height: 100rpx;
  border-radius: 100%;
  margin: 0;
  border: 2rpx solid #d6e8f7;
  box-sizing: border-box;
  display: block;
  overflow: hidden;
}

.g-main-box .m-main-user .m-user-msg {
  width: auto;
  height: auto;
  display: block;
  overflow: hidden;
  margin: 0 0 0 29rpx;
  -webkit-box-flex: 1;
}

.g-main-box .m-main-user .m-user-msg .u-name {
  width: 100%;
  height: 38rpx;
  line-height: 38rpx;
  font-size: 34rpx;
  font-weight: bold;
  color: #333;
  margin: 10rpx 0 0;
  display: block;
  overflow: hidden;
}

.g-main-box .m-main-user .m-user-msg .u-date {
  width: 100%;
  height: 24rpx;
  line-height: 24rpx;
  font-size: 22rpx;
  font-weight: normal;
  color: #888;
  margin: 20rpx 0 0;
  display: block;
  overflow: hidden;
}

.g-main-box .m-main-user .u-code {
  width: auto;
  height: auto;
  margin: 16rpx 0 0;
  display: -webkit-box;
  overflow: hidden;
}

.g-main-box .m-main-user .u-code text {
  width: auto;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 22rpx;
  font-weight: normal;
  color: #888;
  display: block;
  overflow: hidden;
  -webkit-box-flex: 1;
}

.g-main-box .m-main-user .u-code image {
  width: 110rpx;
  height: 30rpx;
  margin: 0 0 0 26rpx;
  display: block;
  overflow: hidden;
}

.g-main-box .u-user-comment {
  width: 100%;
  height: auto;
  line-height: 34rpx;
  font-size: 28rpx;
  font-weight: normal;
  color: #333;
  display: block;
  overflow: hidden;
  padding: 30rpx 0;
}

.g-main-box .m-goods-box {
  width: 100%;
  height: auto;
  padding: 22rpx;
  box-sizing: border-box;
  background: #f8f8f8;
  border-radius: 12rpx;
  display: -webkit-box;
  overflow: hidden;
}

.g-main-box .m-goods-box .u-img {
  width: 125rpx;
  height: 125rpx;
  border-radius: 14rpx;
  display: block;
  overflow: hidden;
}

.g-main-box .m-goods-box .m-goods-msg {
  width: auto;
  height: auto;
  margin: 0 0 0 30rpx;
  display: block;
  overflow: hidden;
  -webkit-box-flex: 1;
}

.g-main-box .m-goods-box .m-goods-msg .u-name {
  width: 100%;
  height: 65rpx;
  line-height: 28rpx;
  font-size: 26rpx;
  font-weight: normal;
  color: #333;
  display: block;
  overflow: hidden;
}

.g-main-box .m-goods-box .m-goods-msg .m-money {
  width: 100%;
  height: auto;
  margin: 4rpx 0 0;
  display: -webkit-box;
  overflow: hidden;
}

.g-main-box .m-goods-box .m-goods-msg .m-money text {
  width: auto;
  height: 56rpx;
  line-height: 78rpx;
  font-size: 30rpx;
  font-weight: bold;
  color: #ff2741;
  display: block;
  overflow: hidden;
  -webkit-box-flex: 1;
}

.g-main-box .m-goods-box .m-goods-msg .m-money .u-btn {
  width: 182rpx;
  height: 56rpx;
  line-height: 56rpx;
  font-size: 24rpx;
  font-weight: normal;
  color: #ff2741;
  text-align: center;
  border: 2rpx solid #ff2741;
  border-radius: 12rpx;
  display: block;
  overflow: hidden;
}

.g-main-box .m-function-btn {
  width: 100%;
  height: auto;
  padding: 30rpx 0 0;
  display: flex;
  overflow: hidden;
}

.g-main-box .m-function-btn .u-btn {
  width: auto;
  height: auto;
  text-align: center;
  display: block;
  overflow: hidden;
  flex: 1;
}

.g-main-box .m-function-btn .u-btn image {
  width: 40rpx;
  height: 36rpx;
  display: inline-block;
  overflow: hidden;
}

.g-main-box .m-function-btn .u-btn text {
  width: auto;
  height: 36rpx;
  line-height: 36rpx;
  font-size: 28rpx;
  font-weight: normal;
  color: #333;
  text-align: center;
  margin: 0 0 0 22rpx;
  display: inline-block;
  overflow: hidden;
}


.g-main-box2 {
  width: 690rpx;
  height: auto;
  background: #fff;
  border-radius: 16rpx;
  box-shadow: 0px 2rpx 16rpx #ccc;
  margin: 36rpx auto 0;
  display: block;
  overflow: hidden;
  position: relative;
}

.g-main-box2 .u-img {
  width: 690rpx;
  height: 300rpx;
  display: block;
  overflow: hidden;
}

.g-main-box2 .m-main-msg {
  width: 100%;
  height: auto;
  padding: 28rpx 25rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #eee;
  display: block;
  overflow: hidden;
}

.g-main-box2 .m-main-msg .u-name {
  width: 100%;
  height: auto;
  line-height: 38rpx;
  font-size: 34rpx;
  font-weight: bold;
  color: #333;
  display: block;
  overflow: hidden;
}

.g-main-box2 .m-main-msg .m-address {
  width: 100%;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 24rpx;
  font-weight: normal;
  color: #888;
  display: -webkit-box;
  overflow: hidden;
  margin: 20rpx 0 0;
}

.g-main-box2 .m-main-msg .m-address .u-font {
  width: auto;
  display: block;
  overflow: hidden;
  -webkit-box-flex: 1;
}

.g-main-box2 .m-main-msg .m-address .u-icon {
  width: auto;
  height: auto;
  display: -webkit-box;
  overflow: hidden;
}

.g-main-box2 .m-main-msg .m-address .u-icon image {
  width: 25rpx;
  height: 30rpx;
  display: block;
  overflow: hidden;
}

.g-main-box2 .m-main-msg .m-address .u-icon text {
  width: auto;
  display: block;
  overflow: hidden;
  -webkit-box-flex: 1;
  margin: 0 0 0 12rpx;
}

.g-main-box2 .m-main-list {
  width: 100%;
  height: auto;
  padding: 2rpx 26rpx 28rpx;
  box-sizing: border-box;
  display: block;
  overflow: hidden;
}

.g-main-box2 .m-main-list .m-main-li {
  width: 100%;
  height: 32rpx;
  line-height: 32rpx;
  font-size: 24rpx;
  font-weight: normal;
  color: #333;
  margin: 26rpx 0 0;
  display: -webkit-box;
  overflow: hidden;
}

.g-main-box2 .m-main-list .m-main-li .u-label {
  width: 32rpx;
  font-weight: normal;
  color: #fff;
  text-align: center;
  background: #f00;
  border-radius: 4rpx;
  display: block;
  overflow: hidden;
}

.g-main-box2 .m-main-list .m-main-li .u-name {
  width: auto;
  margin: 0 12rpx;
  display: block;
  overflow: hidden;
  -webkit-box-flex: 1;
}

.g-main-box2 .m-main-list .m-main-li .u-icon {
  width: 14rpx;
  height: 14rpx;
  border-top: 4rpx solid #999;
  border-right: 4rpx solid #999;
  transform: rotate(45deg);
  display: block;
  overflow: hidden;
  margin: 4rpx 10rpx 0;
}

.g-main-box2 .m-good {
  width: auto;
  height: 55rpx;
  line-height: 55rpx;
  font-size: 24rpx;
  font-weight: normal;
  color: #fff;
  background: rgba(0, 0, 0, 0.5);
  border-radius: 0 0 0 16rpx;
  display: -webkit-box;
  overflow: hidden;
  position: absolute;
  right: 0;
  top: 0;
  z-index: 10;
  padding: 0 26rpx;
}

.g-main-box2 .m-good image {
  width: 24rpx;
  height: 24rpx;
  margin: 16rpx 0 0;
  display: block;
  overflow: hidden;
}

.g-main-box2 .m-good text {
  margin: 0 0 0 12rpx;
  display: block;
  overflow: hidden;
  -webkit-box-flex: 1;
}


