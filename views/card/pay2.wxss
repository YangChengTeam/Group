/* views/card/pay2.wxss */

page{ background: #f7f7f7; padding: 0 0 200rpx 0;}

.g-white-box{ width: 100%; height: auto; background: #fff; display: block; overflow: hidden}

/*选择会员*/
.m-choose-cont{ width: 680rpx; height: 230rpx; border: 1px solid #e7e7e7; box-sizing:border-box; display: -webkit-box; overflow: hidden; margin: 24rpx auto 0; position: relative}
.m-choose-cont .m-left{ width: auto; height: auto; display: block; overflow: hidden; -webkit-box-flex: 1}
.m-choose-cont .m-left .u-t1{ width: 100%; height: 38rpx; line-height: 38rpx; font-size: 36rpx; font-weight: normal; color: #333; margin: 37rpx 0 0 30rpx; display: block; overflow: hidden}
.m-choose-cont .m-left .u-t2{ width: 100%; height: 34rpx; line-height: 34rpx; font-size: 28rpx; font-weight: normal; color: #f32750; margin: 20rpx 0 0 30rpx; display: block; overflow: hidden}
.m-choose-cont .m-right{ width: 225rpx; height: auto; display: block; overflow: hidden}
.m-choose-cont .m-right .u-t1{ width: 100%; height: 46rpx; line-height: 46rpx; font-size: 36rpx; font-weight: bold; color: #f20f3d; margin: 40rpx 0 0; text-align: center; display: block; overflow: hidden}
.m-choose-cont .m-right .u-t2{ width: 100%; height: 30rpx; line-height: 30rpx; font-size: 24rpx; font-weight: normal; color: #aaa; margin: 10rpx 0 0; text-align: center; display: block; overflow: hidden;  text-decoration:line-through}
.m-choose-cont .u-hover{ display: none}
.m-choose-cont .m-bottom{ width: 618rpx; height: 68rpx; line-height: 68rpx; font-size: 24rpx; font-weight: normal; color: #4c4c4c; border-top: 1px dotted #f32750; display: -webkit-box; overflow: hidden; 
position: absolute; left: 30rpx; bottom: 0; z-index: 5}
.m-choose-cont .m-bottom .u-label{ width:30rpx; height: 30rpx; line-height: 30rpx; font-size: 22rpx; margin: 19rpx 15rpx 0 0; background: #f32750; color: #fff; text-align: center; border-radius: 8rpx; display: block; overflow: hidden}
.m-choose-cont .m-bottom .u-label-1{ width:30rpx; height: 30rpx; line-height: 30rpx; font-size: 22rpx; margin: 19rpx 15rpx 0 0; background: #3668f6; color: #fff; text-align: center; border-radius: 8rpx; display: block; overflow: hidden}
.m-choose-cont .m-bottom .u-t{ width: auto; display: block; overflow: hidden; -webkit-box-flex: 1}
.m-choose-cont.f-hover{ border-color: #f32750}
.m-choose-cont.f-hover .u-hover{ width: 54rpx; height: 54rpx; display: block; overflow: hidden; position: absolute; right: 0; bottom: 0; z-index: 10}

.g-tips{ width: 100%; height: auto; padding:30rpx 35rpx; box-sizing: border-box; font-size: 24rpx; font-weight: normal; color: #999; display: block; overflow: hidden}

/*选择地址*/
.g-address-box{ margin-top: 20rpx;}
.m-address-cont{ width: 100%; height: auto; padding: 30rpx; box-sizing: border-box; display: -webkit-box; overflow: hidden}
.m-address-cont .m-left{ width: auto; height: auto; padding: 0 30rpx 0 0; box-sizing: border-box; display: block; overflow: hidden; -webkit-box-flex: 1}
.m-address-cont .m-left .u-t1{ width: 100%; height: auto; font-size: 30rpx; font-weight: normal; color: #000; display: block; overflow: hidden}
.m-address-cont .m-left .u-t2{ width: 100%; height: auto; line-height: 36rpx; font-size: 28rpx; font-weight: normal; color: #c4c4c4; margin: 14rpx 0 0; display: block; overflow: hidden}

.m-address-cont .m-right{ width: 62rpx; height: auto; line-height: 36rpx; font-size: 30rpx; font-weight: normal; color: #aaa; margin: 20rpx 0 0; display: block; overflow: hidden}
.m-address-cont .u-icon{ width: 16rpx; height: 16rpx; border-top: 4rpx solid #aaa; border-right: 4rpx solid #aaa; display: block; overflow: hidden; transform: rotate(45deg); margin: 40rpx 0 0 20rpx;}


.g-btn-box{ width: 100%; height: 140rpx; background: #fff; display: block; overflow: hidden; position: fixed; left: 0; bottom: 0; z-index: 999}
.g-btn-box view{ width: 680rpx; height: 90rpx; line-height: 90rpx; font-size: 32rpx; font-weight: normal; color: #fff; text-align: center; background: #f20f3d; border-radius: 88rpx; margin: 25rpx auto 0; display: block; overflow: hidden}

.g-address-edit{margin-top: 20rpx; padding: 0 35rpx; box-sizing: border-box}
.g-address-edit .m-edit-view{ width: 100%; height: 91rpx; line-height: 90rpx; font-size: 30rpx; font-weight: normal; color: #000; display: -webkit-box; overflow: hidden; border-bottom: 1px solid #eee;}
.g-address-edit .m-edit-view text{ width: 152rpx; display: block; overflow: hidden}
.g-address-edit .m-edit-view input{ width: auto; height: 90rpx; line-height: 90rpx; display: block; overflow: hidden; -webkit-box-flex: 1}